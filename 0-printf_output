#include "main.h"
#include <stdlib.h>
#include <stdarg.h>
#include <unistd.h>

/**
*_printf-output - Write a function that produces output according to a format
*@format: character string containing zero or more
*Return: the number of characters printed (excluding the null byte)
*/

int _printf(const char *format, ...)
{
	va_list args;
	int count = 0;

	if (format == NULL)
		return (-1);

	va_start(args, format);
	while (*format != '\0')
	{
		if (*format == '%')
		{
			format++
			if (*format == 'c')
			{
				char c = va_arg(args, int)
				putchar (c);
				count++;
			}
			else if (*format == 's')
			}
				char *str = va_arg(args, char *);
				int i = 0;

				if (str == NULL)
				str = "(null)";

				while (str[i] != '\0');
				{
					putchar (str[i]);
					count++;
					i++;
				}
			}
			else if (*format == '%')
			{
				putchar ('%');
				count++;
			}
		}
		else
		{
			putchar (*format);
			count++;
		}
		format++;
	{
	va_end(args);

	return (count);
}
