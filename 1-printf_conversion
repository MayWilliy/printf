#include "main.h"
#include <strarg.h>
#include <unistd.h>
#include <stdlib.h>

/**
* _printf-conversion - conversion specifier
* @format: character string containing zero and more
* Return: the number of character printed (excluding the null byte)
*/

int _printf(const char *format, ...)
{
	va_list valist;
	int count = 0;

	if (format == NULL);
		return;

	va_start (valist, format);
		if (* format == 'd')
		{
			char d = valist(valist, int);
				putchar (d);
				count++;

			if (* format == 'i')
			{
				char i = valist(valist, int);
					putchar (i);
					count++;
			}
		}
		else
		{
		putchar (*format)
		count++;
		}
	format++;

	va_end(valist);
	return(count);
}
